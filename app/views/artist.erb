<h1><%= @sel.name %><h1>
<h2>Discography:</h2>
<% @sel.songs.each do | sel | %>
    <h5>
        <a href= "/songs/<%= sel.slug %>"><%= sel.name %>.</a>
    </h5>
<% end %>
<h2>Genres:</h2>
<% @sel.genres.each do | sel | %>
    <h5>
        <a href= "/genres/<%= sel.slug %>"><%= sel.name %>.</a>
    </h5>
<% end %>

