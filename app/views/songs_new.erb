<form method="post" action="/songs">
    <label name="Name">Name</label>
    <input type="text" name="Name" id="Name">

    <label name="Artist Name">Artist Name</label>
    <input type="text" name="artist_name" id="Artist Name">

    <% Genre.all.each do |genre| %>
        <input type="checkbox" name="genre[id]" id="<%= genre.name %>" value="<%= genre.id %>"><%= genre.name %><br>
    <% end %>

    <input type="submit" value="Create">
</form>