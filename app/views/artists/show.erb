<h1>Artist: <%= @artist.name %></h1>
<br>
<h3>Songs:</h3>
<ul>
  <% @artist.songs.each do |song| %>
    <li><a href="/songs/<%= song.id %>"><%= song.name %></a></li>
  <% end %>
</ul>
<br>
<h3>Genre(s):</h3>
<ul>
  <% @artist.genres.each do |genre| %>
    <li><a href="/genres/<%= genre.id %>"><%= genre.name %></a></li>
  <% end %>
</ul>
<br>

