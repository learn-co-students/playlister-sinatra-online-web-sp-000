<h4>Songs:</h4>
<ul>
  <%@artist.songs.each do |song|%>
  <li><%=song.name%></li>
  <%end%>
</ul>

<h4>Genres:</h4>
<ul>
  <%@artist.genres.each do |genre|%>
  <li><%=genre.name%></li>
  <%end%>
</ul>

<ul>
  <%@artist.songs.each do |song|%>
  <li><a href="/songs/<%= song.slug %>"</a></li>
  <%end%>
</ul>

<ul>
  <%@artist.genres.each do |genre|%>
  <li><a href="/genres/<%= genre.slug %>"</a></li>
  <%end%>
</ul>
