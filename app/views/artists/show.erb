<h1>Welcome to <%= @artist.name%>'s Show Page </h1>

<h2> Artist Name: <%= @artist.name %> </h2>
<h2> Songs: </h2>
  <% @artist.songs.each do |song| %>
  <ul>
<li>

  <h3> <a href= '/songs/<%= song.slug %>'><%= song.name %></a></h3>
  <h4>Genres: <a href='/genres/<%= song.genres[0].slug %>'> <%= song.genres[0].name %></h4>
</li>
</ul>
  <% end %>
