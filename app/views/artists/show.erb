<h1><%= @artist.name %> Show Page</h1>

<h2>Artist Songs</h2>
<% @artist.songs.each do |song| %>
	<a href="/songs/<%=song.slug%>"><%= song.name %></a>

<h2>Artist Genres</h2>
	<% song.genres.each do |genre| %>
	  <a href="/genres/<%=genre.slug%>"><%= genre.name %>
	<%end%>
<%end%>









