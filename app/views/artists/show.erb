

<h1>Info on <%= @artist.name %>:</h1>
<br>
<h3> Name: <%= @artist.name %></h3>
<br>
<h3>Your <%= @artist.name %> Songs(s): </h3> 
<br>
<ul> <% @artist.songs.each do |song| %> 
           <li> <a href="/songs/<%= song.slug %>"><%= song.name %></a></li>
        <%end%>
</ul>
<br>
<h3>Genres:</h3>
<br>
<ul> <% @artist.genres.each do |genre| %>
    <li><a href="/genres/<%= genre.slug %>"><%= genre.name %></a></li>
<%end%>
 