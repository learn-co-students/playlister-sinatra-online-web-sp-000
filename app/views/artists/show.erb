<h2><%= @artist.name %></h2>

<h3>Songs</h3>
<% @artist.songs.each do |s| %>
<section>
  <h4>
    <a href="/songs/<%= s.slug %>"><%= s.name %></a>
  </h4>
  <h5>Genres</h5>
    <ul>
      <% s.genres.each do |g| %>
        <li>
          <a href="/genres/<%= g.slug %>"><%= g.name %></a>
        </li>
      <% end %>
    </ul>
</section>
<% end %>