<h1><%= @artist.name %></h1>
<h3>Songs: </h3>
<ul>
<% @artist.songs.each do |song| %>
  <li><%= song.name %></li>
  <a href=<%= "/songs/#{song.slug}" %>>Song</a>
<% end %>
</ul>
<h3>Genres: </h3>
<ul>
<% @artist.genres.each do |genre| %>
  <li><%= genre.name %></li>
  <a href=<%= "/genres/#{genre.slug}" %>>Genre</a>
<% end %>
</ul>
