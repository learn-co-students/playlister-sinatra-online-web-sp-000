<h1>Artist's Name: <%=@artist.name%></h1>
<h1>Artist's Songs:</h1>
<%@artist.songs.each do |song| %>
    <h2><a href="/songs/<%=song.slug%>"><%=song.name%></a></h2>
<%end%>
<br><br>
<h1>Artist's Genres:</h1>
<%@artist.genres.each do |genre| %>
    <h2><a href="/genres/<%=genre.slug%>"><%=genre.name%></a></h2>
<%end%>