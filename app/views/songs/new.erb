<h1>Create A New Song</h1>
<form action="/songs" method="post">
  <p><input type="text" name="song[name]" placeholder="Song Name" id="Name" ></p>
  <p><input type="text" name="song[artist]" placeholder="Song Artist" id="Artist Name" ></p>
  <%Genre.all.each do |genre|%>
    <input type="checkbox" name="genres[]" id="<%=genre.name%>" value="<%=genre.id%>"><%=genre.name%></input>
  <%end%>
  <br></br>
  <input type="submit" name="Create" id="Create">
</form>
