<form method=POST action="/songs">
  Song Name: <input type="text" id="Name" name="song[name]"><br>
  Genre:<br>
  <% @genres.each do |genre| %>
    <input type="checkbox" name="song[genre_ids][]" id="<%= genre.name %>" value="<%= genre.id %>"><%= genre.name %><br>
  <% end %>
  Artist:<br>
  <input type="text" id="Artist Name" name="artist[name]"><br>
  <input type="submit" id="Create" value="Create Song">
</form>
