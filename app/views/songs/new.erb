<form action="/songs" method="post">
    <label for="song[name]">Name:</label>
    <input type="text" name="song[name]" id="song[name]" value="Name">

    <label for="genres[]">New Age Garbage:</label>
    <% @genres.each do |genre| %>
        <input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.id %>"><%=genre.name%>
    <% end %>

    <label for="artist[name]">Artist Name:</label>
    <input type="text" name="artist[name]" id="artist[name]" value="Artist Name">

    <input type="submit" value="Create">
</form>
