<h1>Add a New Song</h1>

<form method="POST" action="/songs"> 
<p>Name: <input type="text" name="song_name" id="Name"></p>
<p>Artist Name: <input type="text" name="artist[name]" id="Artist Name"></p>
<%Genre.all.each do |genre|%>
    <input type="checkbox" id="<%=genre.name%>" name="genre[<%=genre.name%>]">
    <label for="<%=genre.name%>"><%=genre.name%></label>
<%end%>
<input type="submit" value="Create">
</form>