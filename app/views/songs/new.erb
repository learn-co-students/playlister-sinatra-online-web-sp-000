<form action="/songs" method="post">
    <label for="song[name]">Name:</label>
    <input type="text" name="song[name]" id="song[name]">

    <label for="artists[]">New Age Garbage:</label>
    <%@artists.each do |artist|%>
        <input type="checkbox" name="artists[]" id="<%=artist.name%>" value="<%=artist.id%>"><%=artist.name%>
    <%end%>

    <label for="song[artist]">Artist:</label>
    <input type="text" name="song[artist]" id="song[artist]">

    <input type="submit" value="Create">
</form>
