<form action="/songs" method="POST">
  <label for="song_name">Name</label>
  <input type="text" name="song_name" id="Name"><br>


  <label for="artist_name">Artist Name</label>
  <input type="text" name="artist_name" id="Artist Name"><br>


  <% Genre.all.each do |genre| %>
    <genre>
    <%= genre.name%><input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.id %>">
  <% end %>
  <label for="genre_name">Genre Name</label>
  <input type="text" name="genre_name" id="Genre Name"><br>


  <input type="submit" value="Create">

</form>
