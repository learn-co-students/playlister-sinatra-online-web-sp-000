<h1>Create a new Song</h1>
<form action="/songs" method="POST">
  <label for="Name">Name</label>
  <br/>
  <input type="text" name="song[name]" id="Name"></input><br/><br/>
  <label for="Artist Name">Artist Name</label>
  <br/>
  <input type="text" name="artist_name" id="Artist Name"></input><br/><br/>
  <% Genre.all.each do |genre| %>
    <input id="<%= genre.name %>" type="checkbox" name="song[genre_ids][]" value="<%= genre.id %>"><%= genre.name %></input><br/>
  <% end %>
  <br/>
  <input type="submit" value="Create">
</form>
