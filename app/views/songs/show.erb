<% if !!flash[:message] %>
<%= flash[:message] %>
<% end %>

<% if !!flash[:notice] %>
<%= flash[:notice] %>
<% end %>

<h1>Song: <%= @song.name%></h1>
<h2>Artist: <a href ="/artists/<%= @artist.slug %>"><%= @artist.name%></a></h2>
<h2>Genre: </h2>
<%@genres.each do |genre|%>
<a href ="/genres/<%=genre.slug%>"><%= genre.name%></a>
<% end%>
