<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h1>Song's Name: <%=@song.name%></h1>
<h1>Song's Artist: <a href="/artists/<%=@song.artist.slug%>"><%=@song.artist.name%></a></h1>
<h1>Song's Genres:</h1>
<%@song.genres.each do |genre| %>
    <h2><a href="/genres/<%=genre.slug%>"><%=genre.name%></a></h2>
<%end%>