
<h3>Genre Show Page</h3>
<p>Genre Name: <%= @genre.name %></p>
<p>Genre Artist's:</p>
<ul>
   <% @genre.artists.each do |artist| %>
   <li><a href="/artists/<%= artist.slug %>"><%= artist.name %></li></a>
   <% end %>
</ul>
<p>Genre Song's:</p>
<ul>
   <% @genre.songs.each do |song| %>
   <li><a href="/songs/<%= song.slug %>"><%= song.name %></li></a>
   <% end %>
</ul>


