<h4>Artists:</h4>
<ul>
  <%@genre.artists.each do |artist|%>
  <li><%=artist.name%></li>
  <%end%>
</ul>


<h4>Songs:</h4>
<ul>
  <%@genre.songs.each do |song|%>
  <li><%=song.name%></li>
  <%end%>
</ul>

<ul>
  <%@genre.artists.each do |artist|%>
  <li><a href="/artists/<%= artist.slug %>"</a></li>
  <%end%>
</ul>

<ul>
  <%@genre.songs.each do |song|%>
  <li><a href="/songs/<%= song.slug %>"</a></li>
  <%end%>
</ul>
