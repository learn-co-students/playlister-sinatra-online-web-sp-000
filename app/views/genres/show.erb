<h1><%= @genre.name %></h1>
<ul>
    <li><% @genre.artists.each do |artist| %>
        <a href="/artists/<%= artist.slug %>"><%= artist.name %></a>
        <% end %>
    </li>
</ul>
<ul>
    <li><% @genre.songs.each do |song| %>
        <a href="/songs/<%= song.slug %>"><%= song.name %></a>
        <% end %>
    </li>
</ul>