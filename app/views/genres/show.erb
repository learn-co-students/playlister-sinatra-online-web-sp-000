<h2>Genre: <%= @genre.name.capitalize %></h2>
<h2>Artists:</h2>
<ul>
  <% @genre.artists.each do |a| %>
  <li><a href="/artists/<%= a.slug %>"><%= a.name %></li>
  <% end %>
</ul>
<h2>Songs:</h2>
<ul>
  <% @genre.songs.each do |s| %>
  <li><a href="/songs/<%= s.slug %>"><%= s.name %></li>
  <% end %>
</ul>
