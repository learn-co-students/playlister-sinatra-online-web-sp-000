<h1><%= @genre.name %></h1>
<h3>Songs: </h3>
<ul>
<% @genre.songs.each do |song| %>
  <li><%= song.name %></li>
  <a href=<%= "/songs/#{song.slug}" %>>Song</a>
<% end %>
</ul>
<h3>Artists: </h3>
<ul>
<% @genre.artists.each do |artist| %>
  <li><%= artist.name %></li>
  <a href=<%= "/artists/#{artist.slug}" %>>Artist</a>
<% end %>
</ul>
