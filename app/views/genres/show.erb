<h2>Genre Name:</h2>
<h3><%=@genre.name%></h3>
<h2>Artist List:</h2>
<%@genre.artists.each do |artist|%>
<h3><a href=<%="/artists/#{artist.slug}"%>><%=artist.name%></a></h3><br>
<%end%>
<h2>Song List:</h2>
<%@genre.songs.each do |song|%>
<h3><a href=<%="/songs/#{song.slug}"%>><%=song.name%></a></h3><br>
<%end%>