<h4>Artist: </h4>
<h4><%=@artist.name%></h4>
<h4>Song: </h4>
<ul>
<%@artist.songs.each do |song|%>
    <li><h4><a href="/songs/<%=song.slug%>"><%=song.name%></a></h4></li>
<%end%>
</ul>
<h4>Genre: </h4>
<ul>
<%@artist.genres.each do |genre|%>
    <li><h4><a href="/genres/<%=genre.slug%>"><%=genre.name%></a></h4></li>
<%end%>
</ul>