<h1><%=@artist.name%></h1>
<h2>List of Songs</h2>
<% @artist.songs.each do |instance| %>
  <a href="/<%= instance.class.name.downcase.pluralize %>/<%= instance.slug %>"><%= instance.name %></a><br>
<%end%>
<h2>List of Genres</h2>
<% @artist.genres.each do |instance| %>
  <a href="/<%= instance.class.name.downcase.pluralize %>/<%= instance.slug %>"><%= instance.name %></a><br>
<%end%>
